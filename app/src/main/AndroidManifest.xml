<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.manoj.forpitching">

    <application
        android:allowBackup="true"
        android:icon="@drawable/sg"
        android:label="SgChat"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".BasicDetailsActivity" />
        <activity android:name=".DisplayMyDetailsActivity" />
        <activity android:name=".ChatActivity" />
        <activity android:name=".FullSizeImageView" />
        <activity android:name=".AlarmActivity" />
        <activity android:name=".SignUpActivity" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".UsersDisplayActivity"></activity>
    </application>

</manifest>